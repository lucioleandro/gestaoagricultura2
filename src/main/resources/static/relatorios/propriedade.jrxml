	<queryString>
		<![CDATA[select prop.area_agricola, prop.area_total, 
prop.incra, prop.itr,prop.latitude, prop.longitude,
prop.nome, prop.regularizacao_fundiaria,
prop.reserva_legal, prop.tipo_residencia, atveconomica.descricao as atividadeeconomica,
endereco.cep, endereco.complemento,
endereco.logradouro, endereco.numero, endereco.tipo_logradouro,
bairro.nome as bairro, municipio.nome as municipio, municipio.uf,
agricultor.nome as proprietario, agricultor.apelido, agricultor.cpf, agricultor.rg
from agricultura.agri_propriedade prop
left join agricultura.agri_agricultor agricultor on (agricultor.id = prop.agricultor_id)
left join agricultura.agri_endereco endereco on (endereco.id = prop.endereco_id)
left join agricultura.agri_bairro bairro on (bairro.id = endereco.bairro_id)
left join agricultura.agri_municipio municipio on (municipio.id = endereco.municipio_id)
left join agricultura.agri_atividade_economica_agricultor atveagricultor on (atveagricultor.propriedade_id = prop.id)
left join agricultura.agri_atividade_economica atveconomica on (atveconomica.id = atveagricultor.atividade_economica_id)
 where prop.id =   $P{idPropriedade}]]>
	</queryString>
